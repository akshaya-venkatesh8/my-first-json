{
 "$schema": "http://json-schema.org/draft-04/schema#",


     "address":{
       "type":"object",
       "properties":{
		"streetnumber":{
				 "type":"integer"
				},
	        "streetname":{
				"type":"string",
				"minLength":1,
				"maxLength":20
			      },
		"secondaryaddress":{
				"type":"string",
				"pattern": "APT|BSMT|DEPT|FL|FRNT|HNGR|KEY|LBBY|LOT|LOWR|OFC|PH|PIER|REAR|RM|SIDE|SLIP|SPC|STOP|STE|TRLR|UNIT|UPPR|BLDG [0-9]{2}"
			       },
		"city":{
			"type":"string",
			"minLength":1,
			"maxLength":20
		       },
		"statecode":{
		             "type":"string",
			     "pattern":"[A-Z]{2}"
			    },
	   "zipcode":{
			    "type":"string",
			    "pattern":"[0-9]{5}(-[0-9]{4})?"
			  }
		},
   "required": ["streetnumber","streetname","city","statecode","zipcode"]
}
}
